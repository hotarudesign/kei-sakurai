{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve"
  },
  "include": [
    // "next-env.d.ts",
    // "**/*.ts",
    // "**/*.tsx",
    // "pages/Works.jtsx",
    // "pages/Works.tsx",
    "next.config.js",
    "pages/blog/[id].js",
    "pages/index.js",
    "compornents/About/AboutTextBottom.jsx",
    "compornents/About/AboutTextSkill.jsx",
    "compornents/ScrollEvent.jsx",
    "compornents/Pagination.jsx",
    "pages/_app.js",
    "compornents/About/AboutImage.jsx"
  ],
  "exclude": ["node_modules"]
}
